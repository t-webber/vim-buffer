
            println!(
                "{}\t:{:?}\t:{}\t:{}\t:{}",
                self.content,
                self.mode,
                self.cursor.as_value(),
                event.as_key_press_event().unwrap().code,
                event.as_key_press_event().unwrap().modifiers,
            );
